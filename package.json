{
  "name": "i18n-central-storage",
  "version": "2.0.2",
  "description": "parse files to find words that should be translated by pattern and put them to elasticsearch storage ",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --require babel-register --timeout 5000",
    "prepare": "node_modules/babel-cli/bin/babel.js src --out-dir dist"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alertby/i18n-central-storage.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alertby/i18n-central-storage/issues"
  },
  "homepage": "https://github.com/alertby/i18n-central-storage#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-register": "^6.24.1",
    "debug": "^2.6.9",
    "mocha": "^6.2.0",
    "should": "*"
  },
  "dependencies": {
    "async": "^3.2.2",
    "elasticsearch": "^16.3.0",
    "lodash": "^4.17.15",
    "make-plural": "^4.2.0",
    "moment": "^2.24.0"
  }
